<Frame
  name="LeftComponent"
  width="fill_parent" height="fill_parent" minWidth="100" spacing="10"
  resizeTop="false" resizeLeft="false" resizeBottom="false" resizeRight="true"
  borderColor="#37474f"
  backgroundColor="#ffffff">
<Docker width="1w" height="1w" name="docker" backgroundColor="#1c2327">
  <TabButton width="100" height="20" font="noto" fontSize="12" hoverbackgroundColor="#ffffff" backgroundColor="#aaaaaa" activebackgroundColor="#dddddd"/>
  <SplitIcon width="30" height="30" backgroundColor="#37474f" border="4 2 2 2" borderColor="#ffffff"/>
  <DockerSplit>
    <DockerContainer>
      <DockerTab title="Settings">
        <Container name="container" width="fill_parent" height="fill_parent" backgroundColor="#263238" padding="10">
          <Label fontSize="18" font="noto" color="#90a4ae">Frame header</Label>
          <Divider name="divider" backgroundColor="#90a4ae"/>
          <Label color="#90a4ae" font="noto" fontSize="20">Do you like Popcorn?</Label>

          <RadioGroup name="Radio" width="fill_parent" height="wrap_content" margin="0" spacing="10">
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#263238" borderColor="#455a64" border="1"
                activebackgroundColor="#455a64" activeborderColor="#455a64" activeborder="1"/>
              <Label color="#90a4ae" font="noto" fontSize="12">Yes</Label>
            </Container>
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#263238" borderColor="#455a64" border="1"
                activebackgroundColor="#455a64" activeborderColor="#455a64" activeborder="1"/>
              <Label color="#90a4ae" font="noto" fontSize="12">No</Label>
            </Container>
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#263238" borderColor="#455a64" border="1"
                activebackgroundColor="#455a64" activeborderColor="#455a64" activeborder="1"/>
              <Label color="#90a4ae" font="noto" fontSize="12">Maybe</Label>
            </Container>
          </RadioGroup>

          <Button width="fill_parent" height="20" name="button"
            backgroundColor="#37474f" borderColor="#455a64"
            hoverbackgroundColor="#37474f" hoverborderColor="#455a64"
            color="#90a4ae" fontSize="16" font="noto"
            hoverborder="1"
            radius="8" borderRadius="9"
            hoverradius="5" hoverborderRadius="9"
            border="1 1 1 1">Button!</Button>

          <TextBox
            width="fill_parent" height="20"
            backgroundColor="#37474f" borderColor="#455a64"
            border="1" font="noto" fontSize="16" color="#90a4ae"
            hintColor="#263238" hintText="Username"
            radius="6" borderRadius="7"
            padding="0 10"></TextBox>

          <TextBox
            width="fill_parent" height="20"
            backgroundColor="#37474f" borderColor="#455a64"
            border="1" font="noto" fontSize="16" color="#90a4ae"
            hintColor="#263238" hintText="Password"
            radius="6" borderRadius="7"
            padding="0 10" password="true" ></TextBox>

          <Slider
            name="Slider"
            width="fill_parent" height="25" margin="0"
            backgroundColor="#37474f" borderColor="#455a64" border="1"
            minValue="10" maxValue="90" defaultValue="30" stepSize="10" indicatorInside="true">
          <Component
            borderColor="#263238"
            backgroundColor="#455a64"
            width="5" height="fill_parent" margin="0"
            border="1"/>
        </Slider>

        <Label
          padding="0"
          fontSize="12" font="noto" color="#90a4ae">Test label</Label>

        <Container
          width="fill_parent"
          height="100"
          verticalAlign="false" padding="0">

        <ProgressBar
          width="20" height="fill_parent" name="progressBarVertical"
          backgroundColor="#37474f"
          borderColor="#455a64"
          progressColor="#607d8b"
          border="1" padding="0"
          radius="9"
          borderRadius="10"
          minValue="0.25" maxValue="1.25" vertical="true"/>

        <Divider backgroundColor="#455a64" vertical="true"/>

        <ProgressBar
          width="20" height="fill_parent" name="progressBarVerticalReverse"
          backgroundColor="#37474f"
          borderColor="#455a64"
          progressColor="#607d8b"
          border="1" padding="0"
          radius="9"
          borderRadius="10"
          minValue="0.25" maxValue="1.25" vertical="true" reverseProgress="true"/>

        <Divider backgroundColor="#455a64" vertical="true"/>

        <Slider
          backgroundColor="#37474f"
          borderColor="#455a64"
          border="1"
          vertical="true"
          width="25" height="fill_parent" margin="0"
          minValue="10" maxValue="90" defaultValue="30" stepSize="10" indicatorInside="true">
        <Component
          borderColor="#263238"
          backgroundColor="#455a64"
          width="fill_parent" height="5" margin="0"
          border="1"/>
      </Slider>

    </Container>

    <ProgressBar
      width="fill_parent" height="20" name="progressBar"
      backgroundColor="#37474f"
      borderColor="#455a64"
      progressColor="#607d8b"
      border="1"
      radius="9"
      borderRadius="10"
      minValue="0.25" maxValue="1.25"/>

    <ProgressBar
      width="fill_parent" height="20" name="progressBarReverse"
      backgroundColor="#37474f"
      borderColor="#455a64"
      progressColor="#607d8b"
      border="1" padding="0"
      radius="9"
      borderRadius="10"
      minValue="0.25" maxValue="1.25" reverseProgress="true"/>

    <Container width="fill_parent" verticalAlign="false">
      <Label
        width="wrap_content" height="fill_parent"
        fontSize="16" font="noto" color="#90a4ae">Background Color: </Label>

      <ColorPicker
        width="60" height="30" name="backgroundChanger"
        backgroundColor="#455a64"
        borderColor="#455a64"
        border="2"
        radius="2"
        borderRadius="4"/>
    </Container>
  </Container>
</DockerTab>
<DockerTab title="Project Explorer">
  <Component width="fill_parent" height="fill_parent" backgroundColor="#880000"/>
</DockerTab>
</DockerContainer>
<DockerSplit vertical="true">
  <DockerSplit>
    <DockerContainer>
      <DockerTab title="Editor">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#000088"/>
      </DockerTab>
    </DockerContainer>
    <DockerContainer>
      <DockerTab title="Debugger">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#880088"/>
      </DockerTab>
      <DockerTab title="Profiler">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#008888"/>
      </DockerTab>
    </DockerContainer>
  </DockerSplit>
  <DockerContainer>
    <DockerTab title="Console">
      <Component width="fill_parent" height="fill_parent" backgroundColor="#008800"/>
    </DockerTab>
    <DockerTab title="Variables">
      <Component width="fill_parent" height="fill_parent" backgroundColor="#888800"/>
    </DockerTab>
  </DockerContainer>
</DockerSplit>
  </DockerSplit>
</Docker>
</Frame>
