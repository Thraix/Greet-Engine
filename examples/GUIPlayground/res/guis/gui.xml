<Frame
  name="LeftComponent"
  width="fill_parent" height="fill_parent" minWidth="100" spacing="10"
  resizeTop="false" resizeLeft="false" resizeBottom="false" resizeRight="true"
  borderColor="#37474f"
  backgroundColor="#ffffff">
<Docker width="1w" height="1w" name="docker" backgroundColor="#323232" edgeColor="#323232" edgeSize="10" edgeBorder="2" edgeBorderColor="#626262">
  <TabButton width="100" height="25" font="noto" fontSize="12" color="#ffffff" hoverbackgroundColor="#424242" backgroundColor="#323232" activebackgroundColor="#424242" activeborder="0 0 2 0" activeborderColor="#4CAF50"/>
  <SplitIcon width="30" height="30" backgroundColor="#424242" border="5 2 2 2" borderColor="#626262"/>
  <DockerSplit>
    <DockerContainer>
      <DockerTab title="Settings">
        <Container name="container" width="fill_parent" height="fill_parent" backgroundColor="#424242" padding="10">
          <Label
            fontSize="18" font="noto" color="#E0E0E0">Frame header</Label>
          <Divider name="divider" backgroundColor="#E0E0E0"/>
          <Label color="#E0E0E0" font="noto" fontSize="16">Radio Buttons</Label>

          <RadioGroup name="Radio" width="fill_parent" height="wrap_content" margin="0" spacing="10">
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#424242" borderColor="#4CAF50" border="1"
                activebackgroundColor="#424242" activeborderColor="#4CAF50" activeborder="5"/>
              <Label color="#E0E0E0" font="noto" fontSize="12">Yes</Label>
            </Container>
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#424242" borderColor="#4CAF50" border="1"
                activebackgroundColor="#424242" activeborderColor="#4CAF50" activeborder="5"/>
              <Label color="#E0E0E0" font="noto" fontSize="12">No</Label>
            </Container>
            <Container verticalAlign="false">
              <RadioButton width="16" height="16"
                radius="8" activeradius="8"
                backgroundColor="#424242" borderColor="#4CAF50" border="1"
                activebackgroundColor="#424242" activeborderColor="#4CAF50" activeborder="5"/>
              <Label color="#E0E0E0" font="noto" fontSize="12">Maybe</Label>
            </Container>
          </RadioGroup>

          <Button width="fill_parent" height="30" name="button"
            backgroundColor="#4CAF50"
            borderColor="#1B5E20"
            hoverbackgroundColor="#4CAF50"
            border="0 0 2 0"
            hoverborder="2 0 0 0"
            color="#ffffff" fontSize="16" font="noto">Button!</Button>

          <TextBox
            width="200" height="20"
            backgroundColor="#424242" borderColor="#4CAF50"
            border="0 0 1 0" font="noto" fontSize="16" color="#E0E0E0"
            hintColor="#757575" hintText="Username"
            padding="0 0"></TextBox>

          <TextBox
            width="200" height="20"
            backgroundColor="#424242" borderColor="#4CAF50"
            border="0 0 1 0" font="noto" fontSize="16" color="#E0E0E0"
            hintColor="#757575" hintText="Password"
            padding="0 0" password="true" ></TextBox>

          <Slider
            name="Slider"
            backgroundColor="#757575"
            width="fill_parent" height="10" margin="0" radius="5"
            minValue="10" maxValue="90" defaultValue="30" stepSize="10" indicatorInside="true">
          <Component
            backgroundColor="#4CAF50"
            width="20" height="20" margin="0"
            radius="10"
            />
        </Slider>

        <Label
          padding="0"
          fontSize="12" font="noto" color="#E0E0E0">Test label</Label>

        <Container
          width="fill_parent"
          height="100"
          verticalAlign="false" padding="0">

        <ProgressBar
          width="12" height="fill_parent" name="progressBarVertical"
          backgroundColor="#757575"
          progressColor="#4CAF50"
          radius="6"
          minValue="0.25" maxValue="1.25" vertical="true"/>

        <Divider backgroundColor="#757575" vertical="true"/>

        <ProgressBar
          width="12" height="fill_parent" name="progressBarVerticalReverse"
          backgroundColor="#757575"
          progressColor="#4CAF50"
          radius="6"
          minValue="0.25" maxValue="1.25" vertical="true" reverseProgress="true"/>

        <Divider backgroundColor="#757575" vertical="true"/>

        <Slider
          backgroundColor="#757575"
          width="12" height="1w" margin="0" radius="5"
          minValue="10" maxValue="90" defaultValue="30" stepSize="10" indicatorInside="true"
          vertical="true">
        <Component
          backgroundColor="#4CAF50"
          width="20" height="20" margin="0"
          radius="10"
          />
      </Slider>

    </Container>

    <ProgressBar
      width="fill_parent" height="12" name="progressBar"
      backgroundColor="#757575"
      progressColor="#4CAF50"
      radius="6"
      minValue="0.25" maxValue="1.25"/>

    <ProgressBar
      width="fill_parent" height="12" name="progressBarReverse"
      backgroundColor="#757575"
      progressColor="#4CAF50"
      radius="6"
      minValue="0.25" maxValue="1.25" reverseProgress="true"/>

    <Container width="fill_parent" verticalAlign="false">
      <Label width="wrap_content" height="fill_parent" fontSize="16" font="noto" color="#E0E0E0">Background Color: </Label>

      <ColorPicker
        width="48" height="30" name="backgroundChanger"
        backgroundColor="#455a64"
        borderColor="#757575"
        border="1"
        />
    </Container>
    </Container>
  </DockerTab>
  <DockerTab title="Project Explorer">
    <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
  </DockerTab>
</DockerContainer>
<DockerSplit vertical="true">
  <DockerSplit>
    <DockerContainer>
      <DockerTab title="Editor">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
      </DockerTab>
    </DockerContainer>
    <DockerContainer>
      <DockerTab title="Debugger">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
      </DockerTab>
      <DockerTab title="Profiler">
        <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
      </DockerTab>
    </DockerContainer>
  </DockerSplit>
  <DockerContainer>
    <DockerTab title="Console">
      <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
    </DockerTab>
    <DockerTab title="Variables">
      <Component width="fill_parent" height="fill_parent" backgroundColor="#424242"/>
    </DockerTab>
  </DockerContainer>
</DockerSplit>
  </DockerSplit>
</Docker>
</Frame>
