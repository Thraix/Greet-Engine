[Variables]
lighterGray=#626262
lightGray=#424242
darkGray=#323232
primaryColor=#4CAF50
darkPrimaryColor=#4b994e
darkerPrimaryColor=#307c33
textPrimaryColor=#E0E0E0
textSecondaryColor=#757575
transparent=#00000000

[Frame]
width=fill_parent 
height=fill_parent 
spacing=10
backgroundColor=$darkGray

[Container]
backgroundColor=$lightGray

[Label]
font=noto:12
textColor=$textPrimaryColor

[Button]
width=fill_parent
height=30
backgroundColor=$primaryColor
hover-backgroundColor=$darkPrimaryColor
press-backgroundColor=$darkerPrimaryColor
textColor=$textPrimaryColor
font=noto:16

[TextBox]
width=200
height=20
backgroundColor=$lightGray
borderColor=$primaryColor
border=0 0 1 0
font=noto:16
textColor=$textPrimaryColor
hintTextColor=$textSecondaryColor

[ProgressBar]
width=fill_parent
height=12
backgroundColor=$lighterGray
progressColor=$primaryColor
radius=6

[TabButton]
width=100
height=25
font=noto:12
textColor=$textPrimaryColor
backgroundColor=$darkGray
hover-backgroundColor=$lightGray
active-backgroundColor=$lightGray
active-border=0 0 2 0
active-borderColor=$primaryColor
padding="0 5 0 0"

[SplitIcon]
width=30
height=30
backgroundColor=$lightGray
border=5 2 2 2
borderColor=$lighterGray

[ColorPicker]
width=48
height=30
backgroundColor=$primaryColor
borderColor=$lighterGray
border=1

[Divider]
backgroundColor=$lighterGray
width=fill_parent
height=2

[dividerVertical]
width=2
height=fill_parent
vertical=true

[RadioButton]
width=16
height=16
borderRadius=8
backgroundColor=$lightGray
borderColor=$primaryColor
border=2
radius=6
active-backgroundColor=$primaryColor
active-borderColor=$transparent
active-border=0
active-radius=8

[Slider]
backgroundColor=$lighterGray
width=fill_parent
height=16
radius=8
indicatorInside=true

[sliderVertical]
width=16
height=1w
vertical=true

[SliderIndicator]
width=16
height=16
backgroundColor=$primaryColor
radius=8
